{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist",
    "predeploy": [
      "npm run build"
    ],
    "rewrites": [
      {
        "source": "/shootwaste",
        "destination": "/shootwaste.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
